    table1 = table4.filter 'Athlete' (..Is_In ["TULACH Ondrej", "SONKA Antonin"])
    file1 = table1.write (File.new 'C:\\Users\\Ondra\\Desktop\\Code\\PYTHON\\SCMrozdeleni\\rozdeleni.xls') ..Sheet



